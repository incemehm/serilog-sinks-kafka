{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Kafka",
        "Args": {
          "batchSizeLimit": "50",
          "period": "5",
          "bootstrapServers": "xxxxxxx:9091",
          "topic": "logs",
          //"securityProtocol": "SaslPlaintext",
          //"saslMechanism": "ScramSha512",
          //"saslUsername": "",
          //"saslPassword": "",
          "messagetimeoutms": "2000",
          "acks": "-1",
          "lingerms": "0",
          "batchsize": "1"
        }
      }
    ]
  }
}
